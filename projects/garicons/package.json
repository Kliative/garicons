{
  "name": "garicons",
  "version": "0.0.3",
  "peerDependencies": {
    "@angular/common": "^9.1.9",
    "@angular/core": "^9.1.9"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "ng-packagr": "^9.1.5"
  },
  "description": "Open source animated SVG feather icons",
  "main": "karma.conf.js",
  "scripts": {
    "test": "npm run test",
    "build": "ng-packagr -p package.json"
  },
  "ngPackage": {
    "assets": [
      "src/lib/asstets/global.scss"
    ],
    "dest": "../../dist/garicons",
    "lib": {
      "entryFile": "src/public-api.ts"
    }
  },
  "repository": "git+https://github.com/Kliative/garicons.git",
  "keywords": [
    "feather",
    "icons",
    "feather-icons",
    "animated",
    "css",
    "svg",
    "scss",
    "angular"
  ],
  "author": "Tawanda Kanyangarara (@Kliative)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kliative/garicons/issues"
  },
  "homepage": "https://github.com/Kliative/garicons#readme"
}
